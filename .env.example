# Timezone for the Pi-hole container (e.g., America/Chicago, UTC, etc.)
TZ=America/Chicago

# Admin interface password (set your desired password for Pi-hole)
WEBPASSWORD=your_secure_password

# Static IP for the Pi-hole container (e.g., 192.168.x.x)
PIHOLE_IP=192.168.1.100

# Subnet configuration for the Docker network (e.g., 192.168.x.0/24)
SUBNET=192.168.1.0/24
